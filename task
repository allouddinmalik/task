import java.util.Scanner;

public  class minmax{  
    public static void main(String args[]){ 
    Scanner scanner = new Scanner (System.in);    
     System.out.println("Enter a value  ");
     Scanner val1  =new Scanner(System.in);
     int input1 = scanner.nextInt();

    //  System.out.println("YOU ENTERED " + input1);
     System.out.println("Enter a value");
     Scanner val2  =new Scanner(System.in);
     int input2 = scanner.nextInt();
 
       System.out.println("Enter a value");
     Scanner val3  =new Scanner(System.in);
     int input3 = scanner.nextInt();

     System.out.println("The minimum values are as follow");

     int firstmin = Math.min(input1, input2) ;
    //  System.out.println(firstmin);

     int secondmin = Math.min(firstmin, input3) ;
     System.out.println(secondmin);

     System.out.println("The maximum values are as follow");
     
      int firstmax = Math.max(input1, input2) ;
    //  System.out.println(firstmax);

      int secondmax = Math.max(firstmax, input3) ;
     System.out.println(secondmax);

 
    }  
} 
